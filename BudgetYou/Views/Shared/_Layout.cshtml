<!DOCTYPE html>
<html>
<head>
    <!-- Title here -->
    <title>FamBudget - Welcome</title>
    <!-- Description, Keywords and Author -->
    <meta name="description" content="Your description">
    <meta name="keywords" content="Your,Keywords">
    <meta name="author" content="ResponsiveWebInc">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- Styles -->
    <!-- Bootstrap CSS -->
    <link href="~/theme/css/bootstrap.min.css" rel="stylesheet">
    <!-- jQuery gritter -->
    <link href="~/theme/css/jquery.gritter.css" rel="stylesheet">
    <!-- Font awesome CSS -->
    <link href="~/theme/css/font-awesome.min.css" rel="stylesheet">
    <!-- Custom Color CSS -->
    <link href="~/theme/css/less-style.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/theme/css/style.css" rel="stylesheet">

    <link href="~/theme/css/jquery.dataTables.css" rel="stylesheet">

    <!-- Favicon -->
    <link rel="shortcut icon" href="~/theme/img/budgetbulb.png">
    @RenderSection("css", required: false)
</head>
<body>
    <div class="outer">
        <!-- Sidebar starts -->
        <div class="sidebar">
            <div class="sidey">
                <!-- Logo -->
                <div class="logo">
                    <h1>@Html.ActionLink("FamBudget", "Dashboard", "Home", routeValues: null, htmlAttributes: new { id = "loginLink" })</h1>
                </div>
                <!-- Sidebar navigation starts -->
                <div class="sidebar-dropdown"><a href="#" class="br-blue"><i class="fa fa-bars"></i></a></div>
                <div class="side-nav">

                    <div class="side-nav-block">
                        <!-- Sidebar heading -->
                        <h4>Main</h4>
                        <!-- Sidebar links -->
                        <ul class="list-unstyled">
                            </br>
                            <li><a href="@Url.Action("Dashboard", "Home")"><i class="fa fa-home"></i>Dashboard</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "Households")"><i class="fa fa-h-square"></i>HouseHold</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "Accounts")"><i class="fa fa-university"></i>Accounts</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "Transactions")"><i class="fa fa-credit-card"></i>Transactions</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "Budgets")"><i class="fa fa-google-wallet"></i>Budget</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "BudgetItems")"><i class="fa fa-sitemap"></i>BudgetItems</a></li>
                            <br />
                            <li><a href="@Url.Action("Index", "Invitations")"><i class="fa fa-scissors"></i>Invitations</a></li>
                        </ul>
                    </div>
                </div>
                <!-- Sidebar navigation ends -->

            </div>
        </div>
        <!-- Sidebar ends -->
        <!-- Mainbar starts -->
        <div class="mainbar">
            <!-- Mainbar head starts -->
            <div class="main-head">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-4 col-xs-6">
                            <!-- Page title -->
                            <h2><i class="fa fa-desktop lblue"></i> Dashboard</h2>
                        </div>
                        <div class="col-md-3 col-sm-4 col-xs-6">
                           <p>@DateTime.Now.ToString("D")</p>
                        </div>
                        <div class="col-md-3 col-sm-4 hidden-xs">
                            <div class="clearfix"></div>
                        </div>
                        <div class="col-md-3">
                            @Html.Partial("_LoginPartial")
                        </div>
                    </div>
                </div>
            </div>
            @RenderBody()
            <!-- Mainbar head ends -->
        </div>
    </div>
    
