        <script src="~/theme/js/fullcalendar.min.js"></script>

        <!-- Respond JS for IE8 -->
        <script src="~/theme/js/respond.min.js"></script>
        <!-- HTML5 Support for IE -->
        <script src="~/theme/js/html5shiv.js"></script>
