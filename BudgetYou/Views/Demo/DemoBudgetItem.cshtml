</head>
<body>

    <div class="outer">

        <!-- Sidebar starts -->
        <div class="sidebar">

            <div class="sidey">

                <!-- Logo -->
                <div class="logo">

                    <h1>@Html.ActionLink("Budget-You", "Index", "Home", routeValues: null, htmlAttributes: new { id = "loginLink" })</h1>

                </div>

                <!-- Sidebar navigation starts -->

                <div class="sidebar-dropdown"><a href="#" class="br-blue"><i class="fa fa-bars"></i></a></div>

                <div class="side-nav">

                    <div class="side-nav-block">
                        <!-- Sidebar heading -->
                        <h4>Main</h4>
                        <!-- Sidebar links -->
                        <ul class="list-unstyled">
                            <li><a href="@Url.Action("DemoIndex", "Demo")"><i class="fa fa-home"></i>Dashboard</a></li>
                            <li><a href="@Url.Action("DemoAbout", "Demo")"><i class="fa fa-home"></i>About</a></li>
                            <li><a href="@Url.Action("DemoHousehold", "Demo")"><i class="fa fa-h-square"></i>HouseHold</a></li>
                            <li><a href="@Url.Action("DemoAccount", "Demo")"><i class="fa fa-university"></i>Accounts</a></li>
                            <li><a href="@Url.Action("DemoTransaction", "Demo")"><i class="fa fa-credit-card"></i>Transactions</a></li>
                            <li><a href="@Url.Action("DemoBudget", "Demo")"><i class="fa fa-google-wallet"></i>Budget</a></li>
                            <li><a href="@Url.Action("DemoBudgetItem", "Demo")"><i class="fa fa-sitemap"></i>BudgetItems</a></li>
                            <li><a href="@Url.Action("DemoInvitation", "Demo")"><i class="fa fa-scissors"></i>Invitations</a></li>

                        </ul>
                    </div>




                </div>

                <!-- Sidebar navigation ends -->

            </div>
        </div>
        <!-- Sidebar ends -->
        <!-- Mainbar starts -->
        <div class="mainbar">

            <!-- Mainbar head starts -->
            <div class="main-head">

                <div class="container">

                    <div class="row">
                        <div class="col-md-3 col-sm-4 col-xs-6">
                            <!-- Page title -->

                            <h2><i class="fa fa-desktop lblue"></i> Dashboard</h2>
                        </div>

                        <div class="col-md-3 col-sm-4 col-xs-6">
                            <p>@DateTime.Now.ToString("D")</p>
                        </div>

                        <div class="col-md-3 col-sm-4 hidden-xs">

                            <div class="clearfix"></div>
                        </div>


                        <ul class="nav navbar-nav pull-right">
                            <li>@Html.ActionLink("Hello [Your Email/Username]", "DemoManage", "Demo")</li>
                            <li>@Html.ActionLink("Log off", "Index", "Home")</li>

                        </ul>

                    </div>
                </div>

            </div>

            <!-- Mainbar head ends -->
            <div class="main-content">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">

                            <h1>
                                Demo Display only, Not Functional.
                            </h1>
                        </div>
                    </div>
                </div>
                <br />

                <div class="main-content">
                    <div class="container">

                        <div class="page-content">

                            <!-- Heading -->
                            <div class="single-head">
                                <!-- Heading -->
                                <h2 class="pull-left"><i class="fa fa-table orange"></i> Budget Items</h2>

                                <div class="clearfix"></div>
                            </div>

                            <!-- Table Page -->
                            <div class="page-tables">
                                <!-- Table -->
                                <div class="table-responsive">
                                    <table cellpadding="0" cellspacing="0" border="0" id="data-table" width="100%" class="table ">
